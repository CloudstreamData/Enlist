<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>enlistapp</application>
	<version>1</version>

	<static-files>
		<include path="/favicon.ico" />
		<include path="/img/**.gif" />
		<include path="/img/**.png" />
		<include path="/img/**.jpg" />
		<include path="/js/**.js" />
		<include path="/css/**.css" />
		<include path="/css/**.gif" />
		<include path="/mxunit/resources/**" />
		<include path="/mxunit/images/*.gif" />
		<include path="/mxunit/images/*.png" />
		<exclude path="/**.cfml" />
		<exclude path="/**.cfm" />
		<exclude path="/**.cfc" />
	</static-files>
	
	<resource-files>
		<exclude path="/MachII/dashboard/rawimg/icons/**.png" />
	</resource-files>

	<sessions-enabled>true</sessions-enabled>

	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
</appengine-web-app>